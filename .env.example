# Environment variables for ADK Multi-Agent Educator
# Copy this file to .env and fill in your actual values

# Google AI API Key (required)
GOOGLE_API_KEY=your_google_api_key_here

# Agent Configuration
DEFAULT_MODEL=gemini-1.5-flash
TEMPERATURE=0.7
MAX_TOKENS=2048

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Server Configuration
HOST=localhost
PORT=8000
RELOAD=true

# Multi-Agent System Configuration
ENABLE_COLLABORATION=true
MAX_AGENTS_PER_SESSION=3
SESSION_TIMEOUT=1800  # 30 minutes

# Subject-Specific Settings
MATH_DIFFICULTY_LEVELS=elementary,middle,high,college
SCIENCE_SUBJECTS=physics,chemistry,biology,earth_science
MUSIC_TOPICS=theory,history,composition,performance
